//number of neighbours for each vertex
#include<iostream>
#include<vector>

using namespace std;
int main(){
    int n,e,p1,p2;
    vector< vector<int> > list;
    cin>>n;
    cin>>e;
    vector<int>temp[n];
    for(int k=0;k<n;k++){
        list.push_back(temp[k]);
    }
    for( int j=0;j<e;j++){
        cin>>p1;
        cin>>p2;
        list[p1-1].push_back(p2);
        list[p2-1].push_back(p1);
    }
    for(int i=0;i<n;i++){
        cout<<list[i].size()<<endl;
    }
   return 0;
}
                
                
            
        
    
    



